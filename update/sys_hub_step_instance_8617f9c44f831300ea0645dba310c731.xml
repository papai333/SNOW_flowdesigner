  gs.debug('Integration- EndPoint:'+inputs.endpoint+' Method:'+inputs.method+' JSON Body:'+inputs.json);
 var r = new sn_ws.RESTMessageV2(inputs.endpoint,inputs.method);
 for (var k in v_json){
  r.setStringParameterNoEscape(k,v_json[k]);
 }
 var response = r.execute();
 var responseBody = response.getBody();
 var httpStatus = response.getStatusCode();
  gs.debug('Integration- httpStatus:'+httpStatus+' responseBody:'+responseBody);
  outputs.httpCode=httpStatus;
  outputs.httpStatus=responseBody;
  outputs.response=responseBody;
 // outputs.number=responseBody.result.number;
  
})(inputs, outputs);
